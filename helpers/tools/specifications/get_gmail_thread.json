{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "get_gmail_thread": {
    "type": "object",
    "properties": {
      "request": {
      "type": "object",
      "properties": {
        "thread_id": {
        "type": "string",
        "minLength": 1,
        "description": "The ID of the thread to retrieve."
        }
      },
      "required": ["thread_id"]
      },
      "response": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256,
        "description": "A message to indicate whether the message is successfully deleted."
      }
    },
    "required": ["request", "response"]
    }
  },
  "required": ["get_gmail_thread"]
}
  